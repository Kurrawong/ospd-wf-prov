PREFIX mrr: <https://prez.dev/ManifestResourceRoles/>
PREFIX prez: <https://prez.dev/>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX schema: <https://schema.org/>

[]    a prez:Manifest ;
    prof:hasResource 
        [
            prof:hasArtifact "resources/_background/*.ttl" ;
            prof:hasRole mrr:CompleteCatalogueAndResourceLabels ;
            schema:name "Labels files" ;
        ] ,
        [
            prof:hasArtifact "resources/ontologies/*.ttl" ;
            prof:hasRole mrr:ResourceData ;
            schema:name "OSPD Workflow Ontologies" ;
        ] ,
        [
            prof:hasArtifact "resources/validators/*.ttl" ;
            prof:hasRole mrr:ResourceData ;
            schema:name "OSPD SHACL Validators" ;
        ] ,
        [
            prof:hasArtifact "resources/workflows/*.ttl" ;
            prof:hasRole mrr:ResourceData ;
            schema:name "OSPD Workflow 4Ever workflow instances" ;
        ] ,
        [
            prof:hasArtifact "resources/catalogue.ttl" ;
            prof:hasRole mrr:CatalogueData ;
            schema:name "OSPD Workflow Resources Catalogue" ;
        ] ;
.
